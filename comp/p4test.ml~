EXTEND
         expr:
           [ [ x = expr; "+"; y = expr -> x + y
             | x = expr; "-"; y = expr -> x - y ]
           | [ x = expr; "*"; y = expr -> x * y
             | x = expr; "/"; y = expr -> x / y ]
           | [ x = INT -> int_of_string x
             | "("; e = expr; ")" -> e ] ]
         ;
  END;;

2 + 3;;
